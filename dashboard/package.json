    "@types/node": "12.12.12",
    "@types/react": "16.9.12",
    "@types/react-dom": "16.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-layout": "^0.17.1",
    "react-scripts": "3.2.0",
    "react-tradingview-widget": "^1.3.2",
